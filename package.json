    "chalk": "^2.4.2",
    "cross-env": "~5.2.0",
    "jsonwebtoken": "~8.5.0",
    "loggerhythm": "~3.0.3",
    "moment": "~2.24.0",
    "mysql2": "~1.6.5",
    "node-uuid": "^1.4.8",
    "pg": "~7.12.0",
    "pg-hstore": "~2.3.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "~5.18.4",
    "sequelize-typescript": "next",
    "socket.io": "~2.2.0",
    "socket.io-client": "~2.2.0",
    "sqlite3": "~4.1.0",
    "umzug": "~2.2.0"
  },
  "devDependencies": {
    "@essential-projects/eslint-config": "^1.0.0",
    "@essential-projects/iam_contracts": "~3.6.0",
    "@process-engine/ci_tools": "^2.0.0",
    "@process-engine/consumer_api_client": "6.2.0-alpha.1",
    "@process-engine/consumer_api_contracts": "^9.0.0",
    "@process-engine/external_task_api_client": "2.1.0-alpha.1",
    "@process-engine/management_api_client": "^6.1.0",
    "@process-engine/management_api_contracts": "^13.0.0",
    "@process-engine/persistence_api.contracts": "1.1.0-alpha.4",
    "@process-engine/process_engine_contracts": "^46.0.0",
    "@types/bluebird-global": "^3.5.9",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.10",
    "@types/sequelize": "^4.27.28",
