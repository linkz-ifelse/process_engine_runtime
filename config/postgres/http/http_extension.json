{
  "server": {
    "host": "0.0.0.0",
    "port": 8000
  },
  "parseLimit": "50mb",
  "api": {
    "version": "v1"
  },
  "routeConfiguration": {
    "/app*": {
      "refreshOnInvalidToken": true
    }
  },
  "cors": {
    "enabled": true,
    "options": {
      "allowedHeaders": "*",
      "credentials": true,
      "methods": "GET,HEAD,OPTIONS,PUT,PATCH,POST,DELETE",
      "origin": "*",
      "preflightContinue": false
    }
  }
}
